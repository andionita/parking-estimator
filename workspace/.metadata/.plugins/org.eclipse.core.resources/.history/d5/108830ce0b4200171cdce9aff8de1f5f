import pandas as pd

data = pd.read_csv('C:\\Users\\andigenu\\parking\\sfpark\\training_data_sklearn\\essential_traffic.csv', sep=',')

avg_traffic_occ = data.groupby(['DISTRICT'])['AVERAGE_TRAFFIC_OCCUPANCY'].mean()
print(avg_traffic_occ)
avg_vehicle_count = data.groupby(['DISTRICT'])['AVERAGE_VEHICLE_COUNT'].mean()
print(avg_vehicle_count)
median_speed = data.groupby(['DISTRICT'])['MEDIAN_SPEED'].mean()
print(median_speed)
avg_speed = data.groupby(['DISTRICT'])['MEDIAN_SPEED'].mean()
print(avg_speed)


